<% if @post.valid? %>
  $("#post_<%= @post.id %> .card-text ").html("<%= @post.message %>");
  $("#post_<%= @post.id %> .card-text ").removeClass("hidden");
  $("#edit_post_<%= @post.id %>").remove();
  
<% else %>
  $('.error-wraper').html("<%= j render 'shared/error_messages', post: @post %>");
<% end %>
